AAIA,QAAA,WAAA,EAAA,YAJA;AAKA,QAAA,aAAA,EAAA,QALA;AAMA,QAAA,MAAA,EAAA;AANA,OAjEA,EAyEA;AACA,QAAA,YAAA,EAAA,UADA;AAEA,QAAA,WAAA,EAAA,YAFA;AAGA,QAAA,WAAA,EAAA,YAHA;AAIA,QAAA,WAAA,EAAA,YAJA;AAKA,QAAA,aAAA,EAAA,QALA;AAMA,QAAA,MAAA,EAAA;AANA,OAzEA,CAAA;AAkFA,KApFA;AAsFA,IAAA,QAtFA,oBAsFA,IAtFA,EAsFA;AACA,WAAA,WAAA,GAAA,KAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,WAAA,UAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,IAAA,CAAA;AACA,WAAA,eAAA,GAAA,IAAA;AACA,KA1FA;AA4FA,IAAA,UA5FA,sBA4FA,IA5FA,EA4FA;AACA,UAAA,KAAA,GAAA,KAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,MAAA,OAAA,CAAA,6CAAA,CAAA,IAAA,KAAA,MAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA,CAAA;AACA,KA/FA;AAiGA,IAAA,KAjGA,mBAiGA;AAAA;;AACA,WAAA,eAAA,GAAA,KAAA;AACA,MAAA,UAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,KAAA,CAAA,WAAA,CAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,CAAA,CAAA;AACA,OAHA,EAGA,GAHA,CAAA;AAIA,KAvGA;AAyGA,IAAA,IAzGA,kBAyGA;AACA,UAAA,KAAA,WAAA,GAAA,CAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,KAAA,MAAA,CAAA,KAAA,WAAA,CAAA,EAAA,KAAA,UAAA;AACA,OAFA,MAEA;AACA,aAAA,MAAA,CAAA,IAAA,CAAA,KAAA,UAAA;AACA;;AACA,WAAA,KAAA;AACA;AAhHA;AArDA,CAAA","sourcesContent":["<template>\n  <v-data-table\n    :headers=\"headers\"\n    :items=\"agents\"\n    class=\"shadow-xl mx-5 my-5\"\n  >\n    <template v-slot:top>\n      <v-toolbar flat color=\"white\">\n        <v-toolbar-title>Tasks</v-toolbar-title>\n        <v-spacer></v-spacer>\n        <v-dialog v-model=\"dialog_new_task\" max-width=\"500px\">\n          <template v-slot:activator=\"{ on }\">\n            <v-btn  color=\"primary\" depressed v-on=\"on\">\n                <v-icon left>add</v-icon>\n                New Task</v-btn>\n          </template>\n          <v-card>\n              <v-form\n      ref=\"form\"\n      v-model=\"valid\"\n      :lazy-validation=\"lazy\"\n    >\n            <v-card-title>\n              <span class=\"headline\">{{ formTitle }}</span>\n            </v-card-title>\n\n            <v-card-text>\n              <v-container>\n                   \n                <v-row>\n                     \n                  <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                    <v-text-field v-model=\"editedItem.scheduleName\" label=\"Schedule Name\" required :rules=\"nameRules\"></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" sm=\"6\" md=\"8\">\n                    <v-text-field v-model=\"editedItem.nextRunTime\" label=\"Next Run Time\"></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                    <v-text-field v-model=\"editedItem.createdtime\" label=\"Created Time\"></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                    <v-text-field v-model=\"editedItem.lastRunTime \" label=\"Last Run Time\"></v-text-field>\n                  </v-col>\n                 \n                 \n                </v-row>\n                \n              </v-container>\n            </v-card-text>\n\n            <v-card-actions>\n              <v-spacer></v-spacer>\n              <v-btn depressed color=\"primary\" @click=\"save\" >Save</v-btn>\n              <v-btn color=\"blue darken-1\" text @click=\"close\">Cancel</v-btn>\n            </v-card-actions>\n             </v-form>\n          </v-card>\n        </v-dialog>\n      </v-toolbar>\n    </template>\n    <template v-slot:item.action=\"{ item }\">\n      \n      <v-icon\n        small\n        class=\"mr-2\"\n      >\n        play_arrow\n      </v-icon>\n     \n\n      <v-icon\n        small\n        class=\"mr-2\"\n        @click=\"editItem(item)\"\n      >\n        edit\n      </v-icon>\n      <v-icon\n        small\n        @click=\"deleteItem(item)\"\n      >\n        delete\n      </v-icon>\n    </template>\n\n    <template v-slot:no-data>\n      <v-btn color=\"primary\" @click=\"initialize\">Reset</v-btn>\n    </template>\n    \n    <template v-slot:item.lastRunResult=\"{ item }\">\n      <v-icon :class=\"item.lastRunResult\" small left>check</v-icon>\n      <v-p :class=\"item.lastRunResult\" dark>{{ item.lastRunResult }}</v-p>\n    </template>\n\n     <template v-slot:item.status=\"{ item }\">\n      <v-chip :color=\"item.status\" dark>{{ item.status }}</v-chip>\n    </template>\n\n\n  </v-data-table>\n</template>\n\n\n<script>\n  export default {\n    data: () => ({\n      dialog_new_task: false,\n      valid: false,\n      nameRules: [\n        v => !!v || 'Schedule Name is required',\n      ],\n      menu: false,\n      modal: false,\n      headers: [\n        {text: 'Schedule Name', align: 'left', sortable: true, value: 'scheduleName'},\n        { text: 'Created Time', value: 'createdtime' },\n        { text: 'Next Run Time', value: 'nextRunTime' },\n        { text: 'Last Run Time', value: 'lastRunTime' },\n        { text: 'Last Run Result', value: 'lastRunResult' },\n        { text: 'Status', value: 'status' },\n        { text: 'Actions', value: 'action', sortable: false },\n      ],\n      agents: [],\n      editedIndex: -1,\n      editedItem: {\n        scheduleName: '',\n        nextRunTime: '',\n        createdtime :\"\",\n        lastRunTime: '',\n        lastRunResult: '',\n        status:'Deactive'\n      },\n      defaultItem: {\n        scheduleName: '',\n        nextRunTime: '',\n        createdtime :'',\n        lastRunTime: '',\n        lastRunResult: ''\n      },\n    }),\n\n    computed: {\n      formTitle () {\n        return this.editedIndex === -1 ? 'New Task' : 'Edit Task'\n      },\n    },\n\n    watch: {\n      dialog_new_task (val) {\n        val || this.close()\n      },\n    },\n\n    created () {\n      this.initialize()\n    },\n\n    methods: {\n      initialize () {\n        this.agents = [\n          {\n            scheduleName: 'Urgent Tasks',\n            nextRunTime: \"2019.01.17\",\n            createdtime: \"2019.01.17\",\n            lastRunTime: \"2019.01.17\",\n            lastRunResult: \"Succes\",\n            status: \"Runing\",\n          },\n          {\n            scheduleName: 'Routine Tasks',\n            nextRunTime: \"2019.01.17\",\n            createdtime:\"2019.01.15\",\n            lastRunTime: \"2016.04.05\",\n            lastRunResult: \"Succes\",\n            status: \"Runing\",\n          },\n          {\n            scheduleName: 'Lorem Taks',\n            nextRunTime: \"2019.01.17\",\n            createdtime: \"2019.01.14\",\n            lastRunTime: \"2019.01.17\",\n            lastRunResult: \"Fail\",\n            status: \"Suspended\",\n          },\n          {\n            scheduleName: 'Urgent Tasks',\n            nextRunTime: \"2019.01.17\",\n            createdtime: \"2019.01.13\",\n            lastRunTime: \"2019.01.17\",\n            lastRunResult: \"Succes\",\n            status: \"Runing\",\n          },\n          {\n            scheduleName: 'My2019.01.17',\n            nextRunTime: \"2019.01.17\",\n            createdtime: \"2019.01.12\",\n            lastRunTime: \"2015.01.21\",\n            lastRunResult: \"Succes\",\n            status: \"Runing\",\n          },\n          {\n            scheduleName: 'Jelly PC',\n            nextRunTime: \"2019.01.17\",\n            createdtime: \"2019.01.11\",\n            lastRunTime: \"2019.01.17\",\n            lastRunResult: \"Succes\",\n            status: \"Suspended\",\n          },\n          {\n            scheduleName: 'PC Lolli',\n            nextRunTime: \"2019.01.17\",\n            createdtime: \"2019.02.17\",\n            lastRunTime: \"2016.04.05\",\n            lastRunResult: \"Fail\",\n            status: \"Runing\",\n          },\n          {\n            scheduleName: 'HoneyPC',\n            nextRunTime: \"2019.01.17\",\n            createdtime: \"2020.01.17\",\n            lastRunTime: \"2019.04.12\",\n            lastRunResult: \"Fail\",\n            status: \"Runing\",\n          },\n          {\n            scheduleName: 'MAC OS ',\n            nextRunTime: \"2019.01.17\",\n            createdtime: \"2017.01.17\",\n            lastRunTime: \"2019.01.17\",\n            lastRunResult: \"Succes\",\n            status: \"Runing\",\n          },\n          {\n            scheduleName: 'Other PC',\n            nextRunTime: \"2019.01.17\",\n            createdtime: \"2018.01.17\",\n            lastRunTime: \"2019.02.03\",\n            lastRunResult: \"Succes\",\n            status: \"Runing\",\n          },\n        ]\n      },\n\n      editItem (item) {\n        this.editedIndex = this.agents.indexOf(item)\n        this.editedItem = Object.assign({}, item)\n        this.dialog_new_task = true\n      },\n\n      deleteItem (item) {\n        const index = this.agents.indexOf(item)\n        confirm('Are you sure you want to delete this Agent?') && this.agents.splice(index, 1)\n      },\n\n      close () {\n        this.dialog_new_task = false\n        setTimeout(() => {\n          this.editedItem = Object.assign({}, this.defaultItem)\n          this.editedIndex = -1\n        }, 300)\n      },\n\n      save () {\n        if (this.editedIndex > -1) {\n          Object.assign(this.agents[this.editedIndex], this.editedItem)\n        } else {\n          this.agents.push(this.editedItem)\n        }\n        this.close()\n      },\n    },\n  }\n</script>\n\n<style scoped>\n.shadow-xl\t{\n    box-shadow: 0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04);\n}\n.Runing{\n    background-color: #27ae60!important;\n}\n.Suspended{\n    background-color: #c0392b!important;\n}\n.Succes {\n  color: #27ae60!important;\n}\n.Fail{\n  color: #c0392b!important;\n}\n</style>"],"sourceRoot":"src/views"}]}