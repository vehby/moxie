scriptNameRules\"></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" sm=\"6\" md=\"6\">\n                    <v-text-field v-model=\"editedItem.createdtime\" label=\"Created Time\"></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" sm=\"6\" md=\"12\">\n                    <v-text-field v-model=\"editedItem.commandLine\" label=\"Command Line\" required :rules=\"commandLineRules\"></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" sm=\"6\" md=\"12\">\n                    <v-text-field v-model=\"editedItem.description\" label=\"Description\" required :rules=\"descriptionRules\"></v-text-field>\n                  </v-col>\n                    <v-col cols=\"12\" sm=\"6\" md=\"12\">\n                         <v-file-input v-model=\"editedItem.files\" show-size counter multiple label=\"File input\"></v-file-input>\n                    </v-col>\n\n\n                </v-row>\n                \n              </v-container>\n            </v-card-text>\n\n            <v-card-actions>\n              <v-spacer></v-spacer>\n              <v-btn color=\"blue darken-1\" text @click=\"close\">Cancel</v-btn>\n              <v-btn color=\"blue darken-1\" text @click=\"save\" >Save</v-btn>\n            </v-card-actions>\n             </v-form>\n          </v-card>\n        </v-dialog>\n      </v-toolbar>\n    </template>\n    <template v-slot:item.action=\"{ item }\">\n      <v-icon\n        small\n        class=\"mr-2\"\n        @click=\"editItem(item)\"\n      >\n        edit\n      </v-icon>\n      <v-icon\n        small\n        @click=\"deleteItem(item)\"\n      >\n        delete\n      </v-icon>\n      <v-icon\n        small\n        class=\"ml-2\"\n      >\n        get_app\n      </v-icon>\n    </template>\n    <template v-slot:no-data>\n      <v-btn color=\"primary\" @click=\"initialize\">Reset</v-btn>\n    </template>\n\n    <template v-slot:item.status=\"{ item }\">\n      <v-chip :color=\"item.status\" dark>{{ item.status }}</v-chip>\n    </template>\n\n\n  </v-data-table>\n</template>\n\n\n<script>\n  export default {\n    data: () => ({\n      dialog: false,\n      valid: false,\n      scriptNameRules: [\n        v => !!v || 'Script name is required',\n      ],\n      descriptionRules: [\n        v => !!v || 'Description is required',\n      ],\n      commandLineRules: [\n        v => !!v || 'Command line is required',\n      ],\n      date: new Date().toISOString().substr(0, 7),\n      menu: false,\n      modal: false,\n      headers: [\n        {text: 'Script Name', align: 'left', sortable: true, value: 'scriptName'},\n        { text: 'Description', value: 'description' },\n        { text: 'Created Time', value: 'createdtime' },\n        { text: 'Actions', value: 'action', sortable: false },\n      ],\n      agents: [],\n      editedIndex: -1,\n      editedItem: {\n        scriptName: '',\n        createdtime :'',\n        description: '',\n        commandLine:'',\n        files:''\n        \n      },\n      defaultItem: {\n        scriptName: '',\n        createdtime :'',\n        description: '',\n        commandLine:'',\n        files:''\n      },\n    }),\n\n    computed: {\n      formTitle () {\n        return this.editedIndex === -1 ? 'New Script' : 'Edit Script'\n      },\n    },\n\n    watch: {\n      dialog (val) {\n        val || this.close()\n      },\n    },\n\n    created () {\n      this.initialize()\n    },\n\n    methods: {\n      initialize () {\n        this.agents = [\n          {\n            scriptName: 'MyWindows',\n            description: \"Lorem ipsum dolor sit amet, consectetur adipiscing elit.\",\n            createdtime: \"2019.01.17\",\n            status: \"Active\",\n          },\n          {\n            scriptName: 'MyPC',\n            description: \"Lorem ipsum dolor sit amet, consectetur adipiscing elit.\",\n            createdtime:\"2019.01.15\",\n            platform: \"Mac\",\n            status: \"Active\",\n          },\n          {\n            scriptName: 'HomePC',\n            description: \"Lorem ipsum dolor sit amet, consectetur adipiscing elit.\",\n            createdtime: \"2019.01.14\",\n            status: \"Active\",\n          },\n          {\n            scriptName: 'WorkPC',\n            description: \"Lorem ipsum dolor sit amet, consectetur adipiscing elit.\",\n            createdtime: \"2019.01.13\",\n            status: \"Deactive\",\n          },\n          {\n            scriptName: 'MyWindows',\n            description: \"Lorem ipsum dolor sit amet, consectetur adipiscing elit.\",\n            createdtime: \"2019.01.12\",\n            platform: \"Mac\",\n            status: \"Active\",\n          },\n          {\n            scriptName: 'Jelly PC',\n            description: \"Lorem ipsum dolor sit amet, consectetur adipiscing elit.\",\n            createdtime: \"2019.01.11\",\n            status: \"Deactive\",\n          },\n          {\n            scriptName: 'PC Lolli',\n            description: \"Lorem ipsum dolor sit amet, consectetur adipiscing elit.\",\n            createdtime: \"2019.02.17\",\n            platform: \"Mac\",\n            status: \"Active\",\n          },\n          {\n            scriptName: 'HoneyPC',\n            description: \"Lorem ipsum dolor sit amet, consectetur adipiscing elit.\",\n            createdtime: \"2020.01.17\",\n            platform: \"Linux\",\n            status: \"Active\",\n          },\n          {\n            scriptName: 'MAC OS ',\n            description: \"Lorem ipsum dolor sit amet, consectetur adipiscing elit.\",\n            createdtime: \"2017.01.17\",\n            status: \"Active\",\n          },\n          {\n            scriptName: 'Other PC',\n            description: \"Lorem ipsum dolor sit amet, consectetur adipiscing elit.\",\n            createdtime: \"2018.01.17\",\n            status: \"Active\",\n          },\n        ]\n      },\n\n      editItem (item) {\n        this.editedIndex = this.agents.indexOf(item)\n        this.editedItem = Object.assign({}, item)\n        this.dialog = true\n      },\n\n      deleteItem (item) {\n        const index = this.agents.indexOf(item)\n        confirm('Are you sure you want to delete this Agent?') && this.agents.splice(index, 1)\n      },\n\n      close () {\n        this.dialog = false\n        setTimeout(() => {\n          this.editedItem = Object.assign({}, this.defaultItem)\n          this.editedIndex = -1\n        }, 300)\n      },\n\n      save () {\n        if (this.editedIndex > -1) {\n          Object.assign(this.agents[this.editedIndex], this.editedItem)\n        } else {\n          this.agents.push(this.editedItem)\n        }\n        this.close()\n      },\n    },\n  }\n</script>\n\n<style scoped>\n.shadow-xl\t{\n    box-shadow: 0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04);\n}\n.Active{\n    background-color: #27ae60!important;\n}\n.Deactive{\n    background-color: #c0392b!important;\n}\n</style>"],"sourceRoot":"src/views"}]}